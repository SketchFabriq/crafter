{
  "name": "ROS2 Humble Dev Container",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "visioncraft",
  "workspaceFolder": "/home/vscode/ros_ws",
  "mounts": [
    "source=${localEnv:HOME}${localEnv:USERPROFILE}/.bash_history,target=/home/vscode/.bash_history,type=bind"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "GitHub.copilot-chat",
        "GitHub.copilot",
        "ms-vscode.cpptools",
        "ms-python.python",
        "ms-ros.ros",
        "medo64.render-crlf",
        "mhutchie.git-graph",
        "ms-iot.vscode-ros",
        "ms-python.autopep8",
        "ms-python.flake8",
        "ms-python.isort",
        "ms-python.vscode-pylance",
        "njpwerner.autodocstring",
        "VisualStudioExptTeam.vscodeintellicode",
        "jebbs.plantuml"
      ]
    }
  }
}
